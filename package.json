{
  "name": "gatsby-source-wagtail",
  "description": "Gatsby plugin which adds Wagtail Grapple support to Gatsby",
  "version": "0.5.1",
  "author": "NathHorrigan <NathHorrigan@gmail.com>",
  "bugs": {
    "url": "https://github.com/NathHorrigan/gatsby-source-wagtail/issues"
  },
  "main": "index.js",
  "files": [
    "*.js",
    "babel-plugin-remove-graphql-queries/*.js",
    "index.d.ts"
  ],
  "dependencies": {
    "@babel/runtime": "^7.12.1",
    "apollo-boost": "^0.4.9",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link-http": "^1.5.17",
    "apollo-link-ws": "^1.0.20",
    "apollo-utilities": "^1.3.4",
    "babel-plugin-remove-graphql-queries": "^2.9.20",
    "fs-extra": "^8.1.0",
    "gatsby": "^4.25.7",
    "gatsby-source-filesystem": "^2.4.0",
    "gatsby-source-graphql": "^2.7.6",
    "graphql": "^14.7.0",
    "graphql-2-json-schema": "^0.2.0",
    "graphql-request": "^3.3.0",
    "graphql-tag": "^2.11.0",
    "graphql-tools": "^3.1.1",
    "lodash": "^4.17.20",
    "lodash.clonedeep": "4.5.0",
    "lodash.get": "^4.4.2",
    "lodash.merge": "^4.6.2",
    "query-string": "^6.13.6",
    "react": "^16.14.0",
    "subscriptions-transport-ws": "^0.9.18",
    "traverse": "^0.6.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "babel-preset-gatsby-package": "^0.1.4",
    "cross-env": "^5.2.1",
    "prettier": "2.0.5"
  },
  "homepage": "https://github.com/NathHorrigan/gatsby-source-wagtail//#readme",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "graphql",
    "universal"
  ],
  "peerDependencies": {
    "gatsby": "^2.13.77"
  },
  "resolutions": {
    "graphql": "14.5.0"
  },
  "license": "MIT",
  "repository": "https://github.com/NathHorrigan/gatsby-source-wagtail/",
  "scripts": {
    "build": "babel src/ --out-dir . --ignore **/__tests__",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore **/__tests__",
    "format": "prettier --write src/**/*.js",
    "clean": "rm -rf ./*.js"
  }
}
