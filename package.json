{
  "name": "gatsby-source-graphql-universal",
  "description": "Gatsby plugin which adds a third-party GraphQL API to Gatsby GraphQL with client side execution",
  "version": "3.1.3",
  "author": "Birkir Gudjonsson <birkir.gudjonsson@gmail.com>",
  "bugs": {
    "url": "https://github.com/birkir/gatsby-source-graphql-universal/issues"
  },
  "main": "index.js",
  "files": [
    "*.js",
    "index.d.ts"
  ],
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "@types/graphql": "^14.0.4",
    "apollo-boost": "^0.1.23",
    "apollo-cache-inmemory": "^1.6.2",
    "apollo-client": "^2.6.3",
    "apollo-link": "^1.2.12",
    "apollo-link-http": "^1.5.15",
    "apollo-link-http-common": "^0.2.14",
    "apollo-link-ws": "^1.0.18",
    "apollo-utilities": "^1.3.2",
    "gatsby-source-graphql": "^2.0.8",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.10.0",
    "graphql-tools": "^4.0.3",
    "lodash": "^4.17.11",
    "lodash.clonedeep": "4.5.0",
    "lodash.get": "^4.4.2",
    "query-string": "^6.8.1",
    "traverse": "^0.6.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "babel-preset-gatsby-package": "^0.1.3",
    "cross-env": "^5.1.4"
  },
  "homepage": "https://github.com/birkir/gatsby-source-graphql-universal/#readme",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "graphql",
    "universal"
  ],
  "license": "MIT",
  "peerDependencies": {
    "gatsby": ">2.0.0-alpha"
  },
  "repository": "https://github.com/birkir/gatsby-source-graphql-universal",
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore **/__tests__",
    "clean": "rm -rf ./*.js"
  }
}
